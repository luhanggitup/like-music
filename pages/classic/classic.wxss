/* pages/classic/classic.wxss */
.container{
  width: 100;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.header{
  width: 100%;
  display: flex;
  flex-direction: row;
  height: 100rpx;
  align-items: center;
  justify-content: space-between;
  border-top: 1px solid #f5f5f5;
  border-bottom: 1px solid #f5f5f5;
}
.cpsoide{
  margin-left: 20rpx;
  margin-top: 4rpx;
}
.like{
  margin-top: 6rpx;
}
.navi{
  position: absolute;
  bottom: 40rpx;
}
/* 分享按钮 */
.like-container{
  display: flex;
  flex-direction: raw;
  align-items: center;
}
.share-btn {
  margin-top: 20rpx;
  margin-right: 25rpx;
}

.share {
  width: 40rpx;
  height: 40rpx;
}