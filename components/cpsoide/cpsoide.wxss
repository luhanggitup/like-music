/* components/cpside/cpsoide.wxss */
.container{
  height: 60rpx;
  display: inline-flex;
  flex-direction: row;
}
.container-index{
  display: flex;
  flex-direction: row;
  align-items: baseline;
}
.container-index .plain{
  font-size: 32rpx;
}
.container-index .index{
  font-size: 60rpx;
  line-height: 60rpx;
  font-weight: 800;
  margin-right: 14rpx;
}
.container-index .line{
  height: 44rpx;
  border-left: 2rpx solid black;
  margin-right: 14rpx;
}
.container-date{
  display: flex;
  flex-direction: column;
  margin-top: 5rpx;
}
.container-date .month{
  font-size: 24rpx;
  line-height: 24rpx;
}
.container-date .year{
  font-size: 20rpx;
}
